{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "Polymer come in",
  "version": "1.0",

  "browser_action": {
    "default_icon": "extension/icon.png",
    "default_popup": "new.html"
  },
  "permissions": [
    "http://*/",
    "https://*/"
],
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
